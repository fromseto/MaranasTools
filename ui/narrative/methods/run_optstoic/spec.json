{
    "ver": "0.0.1",
    "authors": [
        "lqw5322"
    ],
    "contact": "http://kbase.us/contact-us/",
    "visible": true,
    "categories": ["active", "metabolic_modeling"],
    "widgets": {
        "input": null,
        "output": null
    },
    "parameters": [ 
        {
            "id": "model_upa",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseFBA.FBAModel" ]
            }
        },
        {
            "id": "start_compound_id",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "textsubdata",
            "textsubdata_options": {
                "allow_custom": true,
                "show_src_obj": false,
                "subdata_selection": {
                    "parameter_id": "model_upa",
                    "path_to_subdata": [
                        "modelcompounds"
                    ],
                    "selection_id": "id",
                    "selection_description": [
                        "name"
                    ],
                    "subdata_included": [
                        "modelcompounds/[*]/id",
                        "modelcompounds/[*]/name"
                    ],
                    "description_template": "{{name}}"
                },
                "multiselection": false
            }
        },
        {
            "id": "target_compound_id",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "textsubdata",
            "textsubdata_options": {
                "allow_custom": true,
                "show_src_obj": false,
                "subdata_selection": {
                    "parameter_id": "model_upa",
                    "path_to_subdata": [
                        "modelcompounds"
                    ],
                    "selection_id": "id",
                    "selection_description": [
                        "name"
                    ],
                    "subdata_included": [
                        "modelcompounds/[*]/id",
                        "modelcompounds/[*]/name"
                    ],
                    "description_template": "{{name}}"
                },
                "multiselection": false
            }
        },
        {
            "id": "fixed_stoich",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "field_type": "text",
            "text_options": {
                "validate_as": "float",
                "min_float": 0
            }
        },
        {
            "id": "fixed_stoich2",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "field_type": "text",
            "text_options": {
                "validate_as": "float",
                "min_float": 0
            }
        },
        {
            "id": "integer_stoich",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "field_type": "checkbox",
            "checkbox_options": {
                "checked_value": 1,
                "unchecked_value": 0
            }
        },
        {
            "id": "objective",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["metabolite id"],
            "field_type": "text"
        },
        {
            "id": "max_steps",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["10"],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_int": 1
            }
        },
        {
            "id": "allow_heterologs",
            "optional": false,
            "advanced": true,
            "allow_multiple": false,
            "field_type": "checkbox",
            "checkbox_options": {
                "checked_value": 1,
                "unchecked_value": 0
            }
        },
        {
            "id": "dG_threshold",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "field_type": "text",
            "text_options": {
                "validate_as": "float"
            }
        },
        {
            "id": "num_pathways",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["1"],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_int": 1
            }
        }
    ],
    "parameter-groups": [
        {
            "id": "reactant_stoich",
            "optional": false,
            "allow_multiple": true,
            "parameters": ["start_compound_id", "fixed_stoich"],
            "with_border": true
        },
        {
            "id": "product_stoich",
            "optional": false,
            "allow_multiple": true,
            "parameters": ["target_compound_id", "fixed_stoich2"],
            "with_border": true
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "MaranasTools",
            "method": "run_optstoic",
            "input_mapping": [
                {
                    "input_parameter": "model_upa",
                    "target_property": "model_upa"
                },
                {
                    "input_parameter": "reactant_stoichs",
                    "target_property": "reactant_stoichs"
                },
                {
                    "input_parameter": "product_stoichs",
                    "target_property": "product_stoichs"
                },
                {
                    "input_parameter": "integer_stoich",
                    "target_property": "integer_stoich"
                },
                {
                    "input_parameter": "objective",
                    "target_property": "objective"
                },
                {
                    "input_parameter": "max_steps",
                    "target_property": "max_steps"
                },
                {
                    "input_parameter": "allow_heterologs",
                    "target_property": "use_heterologous_steps"
                },
                {
                    "input_parameter": "dG_threshold",
                    "target_property": "dG_threshold"
                },
                {
                    "input_parameter": "num_pathways",
                    "target_property": "num_pathways"
                },
                {
                  "narrative_system_variable": "workspace",
                  "target_property" : "workspace_name"
                }
            ],
            "output_mapping":[
                {
                    "service_method_output_path": [0, "report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0, "report_ref"],
                    "target_property": "report_ref"
                },
                {
                    "constant_value": "16",
                    "target_property": "report_window_line_height"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}